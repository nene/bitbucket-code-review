{
    "manifest_version": 2,

    "name": "Bitbucket mark-as-read",
    "description": "Mark commits in Bitbucket as read or unread",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": ["https://bitbucket.org/zeroturnaround/xr-agent/commits/*"],
            "js": [
                "jquery.min.js",
                "bitbucket-mark-as-read.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ]
}
